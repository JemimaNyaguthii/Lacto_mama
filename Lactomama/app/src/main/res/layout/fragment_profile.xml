<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.LactationistProfile">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="156dp"
            android:layout_height="266dp"
            android:layout_marginStart="126dp"
            android:layout_marginEnd="127dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/group" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="151dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="145dp"

            android:text="Dr.Roweena Njeri"
            android:textColor="#014B6E"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="Lactationist"

            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="+639076384327"
            android:textColor="#014b6e"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="24dp"
            android:text="Nairobi,Kenya"
            android:textColor="#014b6e"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/btnedit"
            app:layout_constraintEnd_toEndOf="@+id/textView8"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <Button
            android:id="@+id/btnedit"
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#C1EFE6"
            android:gravity="center_vertical"
            android:text="Edit Profile"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <Button
            android:id="@+id/btnbook"
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            android:backgroundTint="#C1EFE6"
            android:gravity="center_vertical"
            android:text="All Bookings"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="@+id/btnedit"
            app:layout_constraintStart_toStartOf="@+id/btnedit"
            app:layout_constraintTop_toBottomOf="@+id/btnedit" />

        <Button
            android:id="@+id/btncourses"
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="#C1EFE6"
            android:gravity="center_vertical"
            android:text="Courses"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="@+id/btnbook"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/btnbook"
            app:layout_constraintTop_toBottomOf="@+id/btnbook" />

        <Button
            android:id="@+id/btnlogout"
            android:layout_width="340dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            android:backgroundTint="#C1EFE6"
            android:gravity="center_vertical"
            android:text="Logout"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="@+id/btncourses"
            app:layout_constraintStart_toStartOf="@+id/btncourses"
            app:layout_constraintTop_toBottomOf="@+id/btncourses" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnedit"
            app:layout_constraintEnd_toEndOf="@+id/btnedit"
            app:layout_constraintTop_toTopOf="@+id/btnedit"
            app:layout_constraintVertical_bias="0.476"
            app:srcCompat="@drawable/baseline_edit_24" />



        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnlogout"
            app:layout_constraintEnd_toEndOf="@+id/btnlogout"
            app:layout_constraintTop_toBottomOf="@+id/btncourses"
            app:layout_constraintVertical_bias="0.675"
            app:srcCompat="@drawable/baseline_logout_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>